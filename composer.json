{
  "name": "weebel/container",
  "description": "A simple and minimal but yet powerful container",
  "license": ["MIT"],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Weebel\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Weebel\\Container\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^6.0",
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.13",
    "mockery/mockery": "^1.5"
  },
  "require": {
    "psr/container": "^2.0",
    "weebel/contracts": "dev-master"
  }
}
